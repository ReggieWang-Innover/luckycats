<script type="Text/Javascript" language="JavaScript">
//<![CDATA[

$.fn.onlyNum = function () {
    $(this).keypress(function (event) {
        var eventObj = event || e;
        var keyCode = eventObj.keyCode || eventObj.which;
        if ((keyCode >= 48 && keyCode <= 57) || keyCode == 46 || keyCode == 8 || keyCode == 9)
            return true;
        else
            return false;
    }).blur(function() {
    	this.value=this.value.replace(/[^\d]/g,'');
    }).bind("paste", function () {
    //获取剪切板的内容
        var clipboard = window.clipboardData.getData("Text");
        if (/^\d+$/.test(clipboard))
            return true;
        else
            return false;
    });
};

function initRadio(name)
{
	updateRadioAddtions(name);
	
	$("input[name='" + name + "']").click(function(){
		updateRadioAddtions(name);
	});
}

function updateRadioAddtions(name)
{
	checkedval = $("input[name='" + name + "']:checked").val();
	$("input[name='" + name + "']").each(function() {
		radio = $(this);
		addsTag = 'J_' + name + '_' + radio.val() + '_adds';
		if (checkedval != null && checkedval == radio.val()) {
			$("span[id='" + addsTag + "']").find(':input').removeAttr('disabled');
		} else {
			$("span[id='" + addsTag + "']").find(':input').attr('disabled', true);
		}
	})
}

function checkSubmitItem(name)
{
	$('#J_' + name).css('color', '#003399');
	$("[id^='" + name + "']").find(':input').css('border-color', '');	

	selectedVal = $("input[name='" + name + "']:checked").val();
	if (selectedVal == null){
		$('#J_' + name).css('color', 'red');
		return false;
	}
	
	missingAdds = false;
	addsTag = 'J_' + name + '_' + selectedVal + '_adds';
	$("span[id='" + addsTag +"']").find(':input').each(function() {
		val = $.trim($(this).val());
		if (val == null || val == '') {
			$(this).css('border-color', 'red');
			if (selectedVal == 'default') {
				$('#J_' + name).css('color', 'red');
			}
			missingAdds = true;
		}
	})
	
	return !missingAdds;
}

function checkPersonInfo()
{
	$("#J_PersonInfo").find(":input").css('border-color', '');
	
	missingInfo = false;
	$("#J_PersonInfo").find(":input").each(function() {
		val = $.trim($(this).val());
		if (val == null || val == '') {
			$(this).css('border-color', 'red');
			missingInfo = true;
		}
	});
	
	return !missingInfo;
}

initPage();
           
function initPage()
{
	$('.numberinput').onlyNum();
	
	for (i = 1; i <= 20; i++)
	{
		initRadio('q' + i);
	}
}

function submitUserSuveryResponse(result)
{
	if (result.errorcode == 'success') {
	} else {
		alert(result.errorcode);
	}
	
	$('J_SuverySubmit').removeAttr('disabled');
	
	location.reload(true);
}

function submitUserSuvery()
{
	firstInvalid = null;
	if (!checkPersonInfo())
	{
		firstInvalid = '#J_PersonInfo';
	}
	
	for (i =1; i <= 20; i++)
	{
		if (!checkSubmitItem('q' + i)){
			if (firstInvalid == null){
				firstInvalid = '#J_q' + i;
			}
		}
	}
	
	if (firstInvalid != null)
	{
		location.hash = firstInvalid;
		return false;
	}
	
	$('J_SuverySubmit').attr('disabled');
	
	$('#J_SuveryForm').ajaxSubmit({
		type:"POST",
		url:"adopt.php",
		cache:false,
		dataType:'json',
		data:'act=submitsurvey'
		success:submitUserSuveryResponse
	});
	
	return false;
}

//]]>
</script>

